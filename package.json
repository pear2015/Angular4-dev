{
  "name": "crms-client",
  "version": "1.4.9-rc",
  "description": "With owlet4-seed in electron",
  "main": "./electron/main.js",
  "scripts": {
    "build": "npm run lint && gulp",
    "build:dev": "npm run lint && gulp dev --dev",
    "build:dist": "npm run lint && gulp dist --release",
    "build:dist:aot": "npm run lint && gulp dist-aot --aot",
    "clean": "npm run clean:dev",
    "clean:dev": "gulp clean",
    "clean:dist": "gulp clean-dist",
    "clean:aot": "gulp clean-aot",
    "start": "npm run build:dev && npm run start-webserver:dev",
    "start:dist": "npm run build:dist && npm run start-webserver:dist",
    "start:dist:aot": "npm run build:dist:aot && npm run start-webserver:dist",
    "start-webserver:dev": "webpack-dev-server --content-base build/ --inline --progress --port 8080",
    "start-webserver:dist": "http-server dist -p 8080 -c-1 --cors",
    "start-electron": "electron ./electron/main.js",
    "preinstall": "npm run install-electron && npm run install-sass",
    "postinstall": "cd ./electron && npm install",
    "install-sass": "npm install node-sass@4.5.0 --SASS_BINARY_SITE=http://172.18.24.51:8081/nexus/content/sites/gs-assets/node/sass/",
    "install-electron": "npm install electron@1.6.11 --ELECTRON_MIRROR=http://172.18.24.51:8081/nexus/content/sites/gs-assets/electron/",
    "watch-sass": "gulp watch-sass",
    "webpack:build": "webpack --progress",
    "webpack:dist": "webpack --config config/webpack/webpack.prod.js --profile --progress",
    "webpack:dist:aot": "cross-env BUILD_AOT=1 && webpack --config config/webpack/webpack.prod.js --progress --profile --bail",
    "test": "karma start karma.conf.js",
    "sonar": "gulp sonar",
    "changelog": "gulp changelog",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "package": "gulp package --release",
    "upload": "gulp upload-to-nexus --release"
  },
  "author": "gsafety",
  "license": "IMT",
  "dependencies": {
