  "engines": {
    "node": ">=5"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run build",
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "themes",
        "i18n",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "build",
        "ci",
        "chore"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    }
  }
}
